{
  "extends": "airbnb-base",
  "env": {
    "es6": true,
    "browser": true,
    "jest": true
  },
  "rules": {
    "no-restricted-syntax": ["error", "LabeledStatement", "WithStatement"],
    "no-console": ["error", { "allow": ["warn", "error", "log"] }]
  },
  "plugins": ["prettier"]
  "eslint linebreak-style": ["error", "windows"]
  "eslint linebreak-style": ["error", "unix"]
}
