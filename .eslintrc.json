{
  "extends": "airbnb-base",
  "env": {
    "es6": true,
    "browser": true,
    "jest": true
  },
  "rules": {
    "no-restricted-syntax": ["error", "LabeledStatement", "WithStatement"],
    "no-console": ["error", { "allow": ["warn", "error", "log"] }],
    "class-methods-use-this": 0,
    "consistent-return": 0,
    "linebreak-style": 0
  },
  "plugins": ["prettier"]
}
