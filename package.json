{
  "name": "shedulizzzer",
  "version": "0.0.1",
  "description": "shedulizzzer app",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "webpack serve --mode development",
    "build": "webpack --mode production",
    "lint": "npm run jslint && npm run lint-html && npm run stylelint",
    "jslint": "eslint .",
    "lint-html": "npx linthtml \"src/*.html\"",
    "stylelint": "stylelint  \"src/css/*.css\"",
    "test": "jest",
    "coverage": "jest --coverage",
    "preshow:coverage": "npm run coverage",
    "show:coverage": "live-server coverage/lcov-report",
    "postinstall": "npx husky install"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aspirants-FS-FE/SheduliZZZer.git"
  },
  "author": "Aspirants FS/FE team",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Aspirants-FS-FE/SheduliZZZer/issues"
  },
  "homepage": "https://github.com/Aspirants-FS-FE/SheduliZZZer#readme",
  "engines": {
    "node": ">= 14.0.0",
    "npm": ">= 6.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@linthtml/linthtml": "^0.6.0",
    "autoprefixer": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.1",
    "css-minimizer-webpack-plugin": "^1.1.5",
    "editorconfig": "^0.15.3",
    "eslint": "^7.17.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-compat": "^3.9.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-webpack-plugin": "^2.4.1",
    "html-loader": "^1.3.0",
    "html-webpack-plugin": "^5.0.0-beta.4",
    "htmllint-loader": "^2.1.4",
    "husky": "^4.3.6",
    "image-minimizer-webpack-plugin": "^2.1.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "live-server": "^1.2.1",
    "mini-css-extract-plugin": "^1.3.3",
    "postcss": "^8.2.2",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^4.1.0",
    "postcss-preset-env": "^6.7.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.8.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-no-unsupported-browser-features": "^4.1.4",
    "stylelint-webpack-plugin": "^2.1.1",
    "webpack": "^5.11.1",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "core-js": "^3.8.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged -r"
    }
  },
  "lint-staged": {
    "*.html": "npm run lint-html",
    "*.js": "npx eslint --cache --fix",
    "*.css": "npx stylelint --fix"
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "not op_mini all",
    "not ie 11",
    "not dead"
  ]
}
