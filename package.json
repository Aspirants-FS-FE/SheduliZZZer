{
  "name": "shedulizzzer",
  "version": "0.0.1",
  "description": "shedulizzzer app",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development",
    "build": "webpack --mode development",
    "lint": "eslint --fix . && linthtml . && stylelint '**/*.css'",
    "jslint": "eslint .",
    "lint-html": "linthtml **/*.html",
    "stylelint": "stylelint **/*.css",
    "test": "jest",
    "coverage": "jest --coverage",
    "preshow:coverage": "npm run coverage",
    "show:coverage": "live-server coverage/lcov-report",
    "preshow:dist": "npm run build",
    "show:dist": "live-server dist",
    "prettier": "prettier --write **/*.{js,css,md,html}",
    "stylelint-check": "stylelint-config-prettier-check",
    "postinstall": "npx husky install",
    "precommit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aspirants-FS-FE/SheduliZZZer.git"
  },
  "author": "Aspirants FS/FE team",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Aspirants-FS-FE/SheduliZZZer/issues"
  },
  "homepage": "https://github.com/Aspirants-FS-FE/SheduliZZZer#readme",
  "devDependencies": {
    "@babel/cli": "^7.11.5",
    "@babel/core": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@linthtml/linthtml": "^0.5.1",
    "babel-jest": "^25.5.1",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.6.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "favicons-webpack-plugin": "^4.2.0",
    "file-loader": "^6.1.1",
    "html-loader": "^1.3.0",
    "html-webpack-plugin": "^4.4.1",
    "husky": "^4.3.0",
    "jest": "^25.5.4",
    "lint-staged": "^10.4.1",
    "live-server": "^1.2.1",
    "mini-css-extract-plugin": "^0.9.0",
    "prettier": "^2.1.2",
    "prettier-stylelint": "^0.4.2",
    "stylelint": "^13.7.2",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "svg-url-loader": "^6.0.0"
  },
  "engines": {
    "node": ">= 14.0.0",
    "npm": ">= 6.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "lint-staged": {
    "**/*.{js,css,md,html}": "prettier --write",
    "**/*.js": "eslint --cache --fix",
    "**/*.css": "stylelint --fix",
    "**/*.html": "linthtml"
  }
}
